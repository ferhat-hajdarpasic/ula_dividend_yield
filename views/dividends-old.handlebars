    <!-- Main jumbotron for a primary marketing message or call to action -->
      <div style="margin-top:0;padding:0" class="jumbotron">
          <div class="container">
              <h1>Recent Dividends History</h1>
              <div class="table-responsive">
                  <table class="table">
                      <tr>
                          <th>Code</th>
                          <th>Company</th>
                          <th>Dividend</th>
                          <th>Ex Dividend Date</th>
                          <th>Record Date</th>
                          <th>Date Payable</th>
                          <th>% Franked</th>
                          <th>Type</th>
                      </tr>
                      <tbody>
                          {{#each dividends}}
                        <tr>
                            <td>{{code}}</td>
                            <td>{{companyName}}</td>
                            <td>{{dividend}}</td>
                            <td>{{formatDate exDividendDate}}</td>
                            <td>{{formatDate recordDate}}</td>
                            <td>{{formatDate datePayable}}</td>
                            <td>{{percentFranked}}</td>
                            <td>{{type}}</td>
                        </tr>
                        {{/each}}
                      </tbody>
                  </table>
              </div>
          </div>
      </div>

          <div class="container">
              <!-- Example row of columns -->

              <div id="myCarousel" class="carousel slide" data-ride="carousel">
                  <!-- Carousel indicators -->
                  <ol class="carousel-indicators">
                      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                      <li data-target="#myCarousel" data-slide-to="1"></li>
                      <li data-target="#myCarousel" data-slide-to="2"></li>
                  </ol>
                  <!-- Carousel items -->
                  <div class="carousel-inner">
                      <div class="active item">
                          <h2>Slide 1</h2>
                          <div class="carousel-caption">
                              <h3>First slide label</h3>
                              {{!<img src="https://plot.ly/~DemoAccount/305.png?width=400&height=400" />}}
                              <div id="chart"></div>
                          </div>
                      </div>
                      <div class="item">
                          <h2>Slide 2</h2>
                          <div class="carousel-caption">
                              <h3>Second slide label</h3>
                              <div id="highchart_container" style="height: 400px; width: 600px"></div>
                          </div>
                      </div>
                      <div class="item">
                          <h2>Slide 3</h2>
                          <div class="carousel-caption">
                              <h3>Third slide label</h3>
                              <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
                          </div>
                      </div>
                  </div>
                  <!-- Carousel nav -->
                  <a class="carousel-control left" href="#myCarousel" data-slide="prev">
                      <span class="glyphicon glyphicon-chevron-left"></span>
                  </a>
                  <a class="carousel-control right" href="#myCarousel" data-slide="next">
                      <span class="glyphicon glyphicon-chevron-right"></span>
                  </a>
              </div>
              <hr>

              <footer>
                  <p>© Company 2014</p>
              </footer>
          </div> <!-- /container -->
          <!-- Bootstrap core JavaScript
        ================================================== -->
          <!-- Placed at the end of the document so the pages load faster -->
          <script src="/jquery.min.js"></script>
          <script src="/bootstrap.min.js"></script>
          <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
          <script src="/ie10-viewport-bug-workaround.js"></script>
            <!--D3-->
          <script src="/d3.v3.js"></script>
            <!--HighCharts-->
            <script src="/highstock.js"></script>
            <script src="/exporting.js"></script>
          <script>
              $(function () {
                  $("#carousel-example-generic").carousel();
              });

              $(function () {
                  var width = 600, height = 400;

                  var color = d3.scale.category20();

                  var force = d3.layout.force()
                      .charge(-120)
                      .linkDistance(30)
                      .size([width, height]);

                  var svg = d3.select("#chart").append("svg")
                      .attr("width", width)
                      .attr("height", height);

                  d3.json("/file/miserables.json", function (error, graph) {
                      graph = $.parseJSON(graph);
                      console.log(graph);
                      force
                          .nodes(graph.nodes)
                          .links(graph.links)
                          .start();

                      var link = svg.selectAll(".link")
                          .data(graph.links)
                        .enter().append("line")
                          .attr("class", "link")
                          .style("stroke-width", function (d) { return Math.sqrt(d.value); });

                      var node = svg.selectAll(".node")
                          .data(graph.nodes)
                        .enter().append("circle")
                          .attr("class", "node")
                          .attr("r", 5)
                          .style("fill", function (d) { return color(d.group); })
                          .call(force.drag);

                      node.append("title")
                          .text(function (d) { return d.name; });

                      force.on("tick", function () {
                          link.attr("x1", function (d) { return d.source.x; })
                              .attr("y1", function (d) { return d.source.y; })
                              .attr("x2", function (d) { return d.target.x; })
                              .attr("y2", function (d) { return d.target.y; });

                          node.attr("cx", function (d) { return d.x; })
                              .attr("cy", function (d) { return d.y; });
                      });
                  });
              });
              $(function () {
                  $.getJSON('/file/appl-c.json', function (data) {
                      data = $.parseJSON(data);
                      console.log(data);
                      // Create the chart
                      $('#highchart_container').highcharts('StockChart', {


                          rangeSelector: {
                              selected: 1
                          },

                          title: {
                              text: 'AAPL Price Chart'
                          },

                          series: [{
                              name: 'AAPL',
                              data: data,
                              tooltip: {
                                  valueDecimals: 2
                              }
                          }]
                      });
                  });

              });

          </script>
</body></html>